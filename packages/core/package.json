{
  "name": "@vtex/ads-core",
  "type": "module",
  "version": "0.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./search": {
      "types": "./dist/clients/search/index.d.ts",
      "import": "./dist/clients/search/index.js",
      "require": "./dist/clients/search/index.js"
    },
    "./adServer": {
      "types": "./dist/clients/adServer/index.d.ts",
      "import": "./dist/clients/adServer/index.js",
      "require": "./dist/clients/adServer/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "adServer": [
        "./dist/clients/adServer/index.d.ts"
      ],
      "search": [
        "./dist/clients/search/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vtex/ads-js.git",
    "directory": "packages/ads-core"
  },
  "homepage": "https://github.com/vtex/ads-js/tree/main/packages/ads-core#readme",
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
